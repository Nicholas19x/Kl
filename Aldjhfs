local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local AutofarmTab = Window:MakeTab({
	Name = "Auto farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local melee = Game.Player.LocalPlayer.PlayerStats.FightingStyle
local sword = Game.Player.LocalPlayer.PlayerStats.SwordName
AutofarmTab:AddDropdown({
	Name = "Weapon",
	Default = "melee",
	Options = {"melee", "sword"},
	Callback = function(Value)
				if Value == "melee" and melee then
			print(melee.Value)
		elseif Value == "sword" and sword then
			print(sword.Value)
		else
	end    
})

